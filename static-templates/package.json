{
  "name": "except-static-templates",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "There's no test command mapped yet.",
    "scss_static": "sass --watch src/assets/scss/bundle.scss:dist/assets/css/bundle.css",
    "scss_wagtail": "sass --watch src/assets/scss/bundle.scss:../except_wagtail/except_wagtail/static/css/except_wagtail.css",
    "pug": "pug -P src/templates -w -o dist",
    "serve": "http-server dist",
    "livereload": "livereload dist",
    "develop": "concurrently --kill-others \"yarn run pug\" \"yarn run livereload\" \"yarn run scss_static\" \"yarn run scss_wagtail\" \"yarn run serve\""
  },
  "author": "GizmoStudios",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "http-server": "^0.11.1",
    "livereload": "^0.7.0",
    "pug-cli": "^1.0.0-alpha6",
    "sass": "^1.17.0"
  }
}